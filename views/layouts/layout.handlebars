<body  >
<div id="preloader">
    <div id="status">&nbsp;</div>
</div>
<div id="loader"></div>
<div id="myDiv">
	<header>
		<h1>Quiz Creator</h1>
    {{#if user}}
        <a href="#">Hi! {{user.username}}</a>
        {{else}}
        <a href="#">Home</a>
        {{/if}}
    </header>

    <ul>

        <li><a href="/users/about">About Us </a></li>
        <li><a href="/users/contact">contact</a></li>
      
          {{#if user}}
       <li><a href="/users/logout">Logout</a></li>
        <li><a href="/users/dashboard">Dashboard</a></li>
        <li><a href="/users/match">Match-Matrix</a></li>
        <li><a href="/users/Inte">Integer-Type</a></li>

        <li><a href="/users/report">MULTIPLE CHOICE</a></li>

        {{else}}
        <li><a href="/users/register">Register</a></li>
        <li><a href="/users/login">Login</a></li>
       {{/if}}
    </ul>


    <div class="main-content">

        
     <div class="row">
        <div class="col-lg-13">
          {{#if success_msg}}
            <div  class="alert alert-success" style="text-align:center">{{success_msg}}</div>
          {{/if}}

          {{#if error_msg}}
            <div class="alert alert-danger" style="text-align:center">{{error_msg}}</div>
          {{/if}}

          {{#if error}}
            <div class="alert alert-danger" style="text-align:center">{{error}}</div>
          {{/if}}
       {{{body}}}

    </div>
    </div>
